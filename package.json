{
  "name": "nucleus",
  "version": "1.0.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "Nucleus",
    "manifest": "src/manifest.json",
    "main": "plugin.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.2",
    "sketch-builder": "^0.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/levtolstoi/Atomary.git"
  },
  "description": "Plugin for create Atomic Design concept in Sketch and export scss-map for front-end"
}
